{
  "expo": {
    "name": "mood-mate",
    "slug": "mood-mate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "mood-mate",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4C9FC1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elhamimdad.moodmate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#4C9FC1"
      },
      "package": "com.elhamimdad.moodmate"
    },
    "web": {
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/push-notification.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification-sound-1.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d9fca98d-da4f-4695-92ec-73049e34abea"
      }
    }
  }
}
